{
	"name": "vite-project",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"generate-api-client": "swagger-typescript-api -p https://raw.githubusercontent.com/pagopa/p4pa-pu-bff/refs/heads/develop/openapi/generated.openapi.json -o ./generated -n apiClient --axios",
		"generate-api-contracts": "swagger-typescript-api  --modular --no-client -p https://raw.githubusercontent.com/pagopa/p4pa-pu-bff/refs/heads/develop/openapi/generated.openapi.json -o ./generated",
		"generate-fileshare-client": "swagger-typescript-api -p https://raw.githubusercontent.com/pagopa/p4pa-fileshare/refs/heads/develop/openapi/p4pa-fileshare.openapi.yaml -o ./generated/fileshare --axios -n fileshareClient --api-class-name FileshareApi",
		"generate-fileshare-contracts": "swagger-typescript-api --modular --no-client -p https://raw.githubusercontent.com/pagopa/p4pa-fileshare/refs/heads/develop/openapi/p4pa-fileshare.openapi.yaml -o ./generated/fileshare",
		"generate-zod-schema": "yarn ts-to-zod --all",
		"generate": "yarn generate-api-client && yarn generate-api-contracts && yarn generate-fileshare-client && yarn generate-fileshare-contracts && yarn generate-zod-schema",
		"dev": "yarn generate && vite",
		"build": "yarn generate && tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"test": "vitest"
	},
	"dependencies": {
		"@emotion/react": "^11.13.5",
		"@emotion/styled": "^11.13.5",
		"@mui/icons-material": "^5.15.15",
		"@mui/lab": "^6.0.0-beta.27",
		"@mui/material": "^5.15.15",
		"@mui/x-data-grid": "^7.23.5",
		"@mui/x-date-pickers": "^7.26.0",
		"@pagopa/mui-italia": "^1.5.1-0",
		"@preact/signals-react": "^2.2.0",
		"@tanstack/react-query": "^5.40.0",
		"axios": "^1.6.8",
		"date-fns": "^4.1.0",
		"i18next": "^23.10.1",
		"i18next-browser-languagedetector": "^7.2.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-i18next": "^14.1.0",
		"react-markdown": "^9.0.3",
		"react-router": "^7.0.0",
		"react-router-dom": "^7.0.0",
		"rehype-sanitize": "^6.0.0",
		"whatwg-fetch": "^3.6.20"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/react": "^16.2.0",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^22.9.1",
		"@types/react": "^19.0.6",
		"@types/react-dom": "^19.0.3",
		"@vitejs/plugin-react": "^4.3.3",
		"@vitest/coverage-v8": "^3.0.5",
		"dotenv": "^16.4.5",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-react-refresh": "^0.4.14",
		"globals": "^15.11.0",
		"jsdom": "^25.0.1",
		"prettier": "^3.2.5",
		"process": "^0.11.10",
		"rimraf": "^5.0.5",
		"swagger-typescript-api": "^13.0.3",
		"ts-node": "^10.9.2",
		"ts-to-zod": "^3.8.5",
		"tsconfig.json": "^1.0.11",
		"typescript": "~5.6.2",
		"typescript-eslint": "^8.11.0",
		"vite": "^5.4.10",
		"vitest": "^3.0.5",
		"vitest-dom": "^0.1.1",
		"zodock": "^0.1.0"
	}
}
