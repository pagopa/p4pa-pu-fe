{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generate-api-client": "swagger-typescript-api -p https://raw.githubusercontent.com/pagopa/p4pa-pu-bff/refs/heads/develop/openapi/p4pa-pu-bff.openapi.yaml -o ./generated -n apiClient --axios",
    "generate-data-contracts": "swagger-typescript-api  --modular --no-client -p https://raw.githubusercontent.com/pagopa/p4pa-pu-bff/refs/heads/develop/openapi/p4pa-pu-bff.openapi.yaml -o ./generated",
    "generate-zod-schema": "yarn ts-to-zod --all",
    "generate": "yarn generate-api-client && yarn generate-data-contracts && yarn generate-zod-schema",
    "dev": "vite",
    "build": "yarn generate && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.15",
    "@pagopa/mui-italia": "^1.5.1-0",
    "@preact/signals-react": "^2.2.0",
    "@tanstack/react-query": "^5.40.0",
    "axios": "^1.6.8",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.0",
    "react-router": "^7.0.0",
    "react-router-dom": "^7.0.0",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "rimraf": "^5.0.5",
    "swagger-typescript-api": "^13.0.3",
    "ts-node": "^10.9.2",
    "ts-to-zod": "^3.8.5",
    "tsconfig.json": "^1.0.11",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "zodock": "^0.1.0"
  }
}
